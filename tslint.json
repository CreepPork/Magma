{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
        "array-type": [
            true,
            "array"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "ban-comma-operator": true,
        "binary-expression-operand-order": true,
        "class-name": true,
        "comment-format": [
            true,
            "check-space",
            "check-uppercase"
        ],
        "curly": true,
        "encoding": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "linebreak-style": [
            true,
            "LF"
        ],
        "label-position": true,
        "file-name-casing": [
            true,
            "camel-case"
        ],
        "newline-before-return": true,
        "no-console": false,
        "no-namespace": true,
        "no-null-keyword": true,
        "no-var-keyword": true,
        "no-var-requires": false,
        "number-literal-format": true,
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ]
    },
    "rulesDirectory": []
}
